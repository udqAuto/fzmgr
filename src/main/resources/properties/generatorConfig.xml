            <property name="enableSubPackages" value="true"/>  
        </javaModelGenerator>  
          
        <!--对应的xml mapper文件  	-->
        <sqlMapGenerator  targetPackage="/mapper/base" targetProject="E:\Workspaces\fzmgr Maven Webapp\src\main\resources">  
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>  
          
          
        <!-- 对应的dao接口  --> 
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.udianqu.wash.dao" targetProject="E:\Workspaces\fzmgr Maven Webapp\src\main\java">  
            <property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>  

 
